{
    "name": "postitief/bolt-contact-form",
    "description": "AJAX Contact Form",
    "type": "bolt-extension",
    "keywords": [
        "postitief", "contact-form", "contact-form-ajax", "contact", "dybo"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dybo",
            "email": "d.boertje@postitief.nl"
        }
    ],
    "minimum-stability": "1.0.*",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Postitief\\ContactForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Postitief\\ContactForm\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Postitief\\ContactForm\\ContactForm"
    }
}
